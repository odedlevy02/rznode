{
  "name": "generator-rznode",
  "version": "1.0.23",
  "description":
    "Creates a basic node app with express and typescript including all dependencies",
  "homepage": "",
  "author": {
    "name": "Oded Levy",
    "email": "odedlevy02@gmail.com",
    "url": ""
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": ["node", "typescript", "yeoman-generator"],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^2.13.1",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^22.0.6"
  },
  "dependencies": {
    "@types/node": "^9.3.0",
    "@types/yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "deep-extend": "^0.6.0",
    "lodash": "^4.17.10",
    "pluralize": "^7.0.0",
    "yeoman-generator": "^2.0.5",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepare": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest",
    "bump:patch": "bump --patch",
    "gen:publish": "npm publish",
    "publish:rznode": "npm run bump:patch && npm run gen:publish"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "license": "Apache-2.0"
}
